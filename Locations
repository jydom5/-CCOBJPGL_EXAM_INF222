package Locations;



import Tourist.Tourist;

public interface Locations {

    int airFare = 1000;

    void accept(Tourist tourist);
}



package Locations;

import Tourist.Tourist;

public class Batanes implements Locations {

    public int airFare = 200;


    public void accept(Tourist tourist) {
        tourist.visit(this);
    }
}



